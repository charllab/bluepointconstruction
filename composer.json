{
    "require": {
        "craftcms/cms": "3.3.0.1",
        "vlucas/phpdotenv": "^2.4.0",
        "craftcms/aws-s3": "1.2.3",
        "craftcms/element-api": "2.5.4",
        "craftcms/redactor": "2.3.3.2",
        "verbb/super-table": "2.1.20",
        "craftcms/feed-me": "4.1.0",
        "craftcms/ckeditor": "1.0.0-beta.2",
        "solspace/craft-express-forms": "1.0.2",
        "solspace/craft-freeform": "3.3.1",
        "putyourlightson/craft-dashboard-begone": "1.0.1",
        "olivierbon/craft-concierge": "2.1.0"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
